<h1>Awesome Garage</h1>

<div class="car-card">
  <% @cars.each do |car| %>
    <h3><%= link_to car.brand, car_path(car) %></h3>
    <h4><%= car.model %></h4>
    <p><%= car.year %></p>
    <p><%= car.fuel %></p>
    <p><%= car.created_at %></p>
    <p><%= car.owner.nickname %></p>
  <% end %>
</div>
